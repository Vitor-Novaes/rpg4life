<% @jogador.errors.full_messages.each do |message|%>
  <div class = "alert alert-danger" role = "alert">
    <%= message%>
  </div>
<% end %>

<%= form_for @jogador do |f| %>
  <h3 id = "estiloDoFormPlayers">Nome do jogador</h3>
  <%= f.text_field :nome%>
  <h3 id = "estiloDoFormPlayers">Nome do personagem</h3>
  <%= f.text_field :personagem%>
  <%= f.submit texto_submit%>
<% end %>
